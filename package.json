{
	"name": "@skyquest/realskyquest-github-io",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"deploy": "vite build && gh-pages -d dist",
		"cdn": "pnpm publish dist --no-git-checks --access public",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check src/**/*.{ts,svelte} && eslint src/**/*.{ts,svelte}",
		"format": "prettier --write src/**/*.{ts,svelte}"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@tailwindcss/typography": "^0.5.10",
		"@tsconfig/svelte": "^5.0.2",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"autoprefixer": "^10.4.16",
		"cssnano": "^6.0.2",
		"daisyui": "^4.4.23",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"gh-pages": "^6.1.0",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^5.0.8",
		"vite-imagetools": "^6.2.8",
		"vite-plugin-tailwind-purgecss": "^0.2.0"
	},
	"dependencies": {
		"lucide-svelte": "^0.302.0"
	}
}
